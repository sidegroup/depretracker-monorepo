<p style="color: green"><%= notice %></p>

<div class="row">
  <div class="col">
    <h1>Examples</h1>
  </div>
</div>

<div class="row">
  <div class="col">
    <div id="examples">
      <% @examples.each do |example| %>
        <%= render example %>
        <p>
          <%= link_to "Show this example", example %>
        </p>
      <% end %>
    </div>

    <%= link_to "New example", new_example_path %>
  </div>
</div>


